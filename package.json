{
  "version": "0.0.0-development",
  "name": "lbh-frontend-repository",
  "description": "Taken from the GDS UK Frontend and used for the development of Hackney Council Frontend only, see `package/package.json` for the published `package.json`",
  "engines": {
    "node": ">=12.14.0"
  },
  "license": "MIT",
  "scripts": {
    "build:sass": "sass lbh/all.scss dist/lbh-frontend.min.css --load-path=./",
    "build:js": "parcel build lbh/all.js --out-dir dist --global LBHFrontend",
    "build:sassdoc": "sassdoc lbh --dest static/sassdoc",
    "build:docs": "docusaurus build",
    "dev": "docusaurus start",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "build": "npm-run-all build:*",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "devDependencies": {
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
    "@mdx-js/react": "^1.6.21",
    "docusaurus-image-loader": "^1.0.1",
    "docusaurus-plugin-sass": "^0.1.11",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "sassdoc": "^2.7.0",
    "semantic-release": "^17.3.9",
    "standard": "^13.1.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "browserslist": [
    ">0.1%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Samsung versions",
    "Safari >= 9",
    "ie 8-11",
    "iOS >= 9"
  ],
  "standard": {
    "globals": [
      "page",
      "browser",
      "context"
    ],
    "ignore": [
      "/dist/**/*.js",
      "/package/**/*.js",
      "/src/vendor/polyfills/**/*.js"
    ]
  },
  "dependencies": {
    "govuk-frontend": "^3.5.0",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "sass": "^1.32.8",
    "underscore": "^1.9.1"
  },
  "sasslintConfig": "config/.sass-lint.yml",
  "repository": {
    "type": "git",
    "url": "https://github.com/LBHackney-IT/LBH-frontend.git"
  }
}
